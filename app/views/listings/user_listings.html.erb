<h4><%= link_to @user.name, user_path %></h4>

<h4><%= @user.listings.count %> listings</h4>

<% @user.listings.each do |listing| %>
	<%= link_to listing_path(listing), target: "_blank" do %>
		<div>
			RM<%= number_with_precision(listing.price_per_night, precision: 2) %>
			<%= listing.title %>
		</div>
	<% end %>
<% end %>